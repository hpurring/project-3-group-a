 userData.users; \n    }\n\n    const renderContactList = () => {\n      if (myContacts.length > 0) {\n        console.log(myContacts)\n        return (\n          <ContactList username={myUsername} contacts={myContacts} />\n        )            \n      }      \n    }\n\n    const handleClick = async (user) => {\n      console.log(user._id)\n        try {\n          await addContact({\n            variables: { _id: user._id }\n          });\n          myContacts = myData.me.contacts;\n          window.location.assign('/contacts');\n          \n        } catch (e) {\n          console.log(e)\n        }\n      };\n    if (loading) {\n      return (\n        <h4>Loading...</h4>\n      )\n    }\n    if (!loading) {\n      allUsers.map(user => console.log(user.cards[0]))\n      return (\n        <div className=\"Search\">\n          <center>\n          <input type=\"text\" \n          placeholder=\"Search Contacts...\" \n          onChange={event => {setSearchTerm(event.target.value);\n          }}\n          />\n          <hr/>\n          \n          {allUsers.filter(user => {\n              if (searchTerm ===\"\") {\n                return ('') \n              } else if ((user.username).toLowerCase().includes(searchTerm.toLowerCase())) {\n                  return user\n              } \n          }).map((user)=> {\n              return (\n              <div className=\"user\" key={user.username}> \n                  <Card data= {user.cards[0]}/>\n               \n                {user.username === myUsername ? '': \n                <button onClick={()=>handleClick(user)}>Add Contact</button>\n                }\n              </div>\n              );\n          })}\n          \n          {renderContactList()}\n\n\n          </center>\n      </div>\n      );      \n    }\n\n}\n\nexport default Search ;","/Users/sihayaharris/Desktop/project-3-group-a/client/src/utils/firebase.js",[],"/Users/sihayaharris/Desktop/project-3-group-a/client/src/components/Card/index.js",[],"/U